/**app.wxss**/
@import "../../template/reportList/reportList.wxss";
.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
    -webkit-text-size-adjust:none;
    -webkit-user-select: none;
    font-size: 35rpx;
    color: #333;
    font-family: Helvetica,Arial,"Helvetica Neue","Droid Sans","Microsoft YaHei",sans-serif;
}
.contain{
  background: #F5F5F5;
}
.canvas {
    width: 100%;
    height: 200px;
    background: #fff;
}
.TabBox{
  width: 100%;
  height: 150rpx;
  background: #3399ff;
  margin: 0;
  padding: 5rpx 15rpx;
  display: flex;
  flex-direction: column;
  text-align: center;
  font-size: 22rpx;
  font-weight: 600;
  color: #fff;
}
.groupTab{
  display: flex;
  flex-direction: row;
}
.groupTab1{
  width: 208rpx;
  margin-bottom: 15rpx;
}
.groupTab2{
  width: 312rpx;
}
.small{
  margin-top:0rpx; 
  line-height: 150rpx;
  font-size: 50rpx;
}
.groupTab view{
  width: 80rpx;
  height: 50rpx;
  line-height: 50rpx;
  padding: 5rpx 12rpx;
  border:1px solid #fff;
}
.groupTab .right{
  border-top-left-radius: 25rpx;
  border-bottom-left-radius: 25rpx;
}
.groupTab .left{
  border-left: 1px solid #fff;
  border-top-right-radius: 25rpx;
  border-bottom-right-radius: 25rpx;
}
.changeBGFFF{
  background: #fff;
  color: #3399ff;
}
.comman{
  background: #3399ff;
  color: #fff;
}
.swiper-box{ 
  display: block; 
  height: 100%; 
  width: 100%; 
  overflow: hidden; 
}  
.swiper-box view{  
    text-align: center;  
} 
.marTop28{
  margin-top: 28rpx;
}
.hidden{
  display: none;
}
.DateBox{
  position: absolute;
  top: 0;
  right:40rpx;
  font-weight: 0;
  font-size: 100rpx;
  line-height: 150rpx;
}
.select_img{
  width: 0;
  height: 0;
  border: 12rpx solid;
  border-top-color: #fff;
  border-bottom-color: transparent;
  border-right-color: transparent;
  border-left-color: transparent;
  position: absolute;
  bottom: 25rpx;
  right: -25rpx;
}