/* pages/report/modify/modify.wxss */
page{
  background: #eee;
}
.session{
  text-align: left;
  width: 670rpx;
  padding: 25rpx 25rpx 30rpx 25rpx;
  background: #fff;
}
.session .inputTitle{
  color: #1FBBA6;
}
.session input{
  border-bottom: 1rpx #eee solid;

}
.session picker{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1rpx #eee solid; 
}

button{
  width: 700rpx;
  height: 90rpx;
  margin: 35rpx auto 0;
  background: #F9AA33 !important;
  color: #fff;
  line-height: 90rpx;
  border-radius: 45rpx;
}